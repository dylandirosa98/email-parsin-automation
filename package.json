{
  "name": "email-parser-crm",
  "version": "1.0.0",
  "description": "Email parsing automation service that monitors Gmail for structured lead emails and creates leads in Twenty CRM",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "test": "node scripts/test-local.js",
    "verify": "node scripts/verify-setup.js",
    "prestart": "npm run build"
  },
  "keywords": [
    "email",
    "parsing",
    "crm",
    "automation",
    "gmail",
    "twenty"
  ],
  "author": "Dylan DiRosa",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "graphql-request": "^6.1.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
